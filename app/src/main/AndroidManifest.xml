<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.indaco.daggertestapp">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DaggerTestApp">
        <activity android:name=".ui.screens.launcher.EmptyActivity"></activity>
        <activity android:name=".ui.screens.launcher.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.screens.signup.SignUpActivity" />
        <activity android:name=".ui.screens.login.LoginActivity" />
        <activity android:name=".ui.screens.welcome.WelcomeActivity" />
        <activity android:name=".ui.screens.landing.LandingActivity" />
    </application>

</manifest>